{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
//        "eslint:recommended",
//        "plugin:react/recommended",
        "prettier"
//        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "eqeqeq": "error",
        "jsx-a11y/accessible-emoji": "off",
        "radix": "off",
        "react/prop-types": "off"
    },
    "globals": {
        "cy": true,
        "Cypress": true
    }
}
